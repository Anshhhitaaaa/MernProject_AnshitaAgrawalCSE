# Server Configuration
PORT=5000

# Database Configuration
# For development: mongodb://localhost:27017/your_database_name
# For production: mongodb+srv://<username>:<password>@cluster.mongodb.net/your_database_name
MONGODB_URI=mongodb://localhost:27017/upgenz

# JWT Configuration
# Generate a strong random string for JWT_SECRET
JWT_SECRET=replace_with_your_secure_jwt_secret_key
JWT_EXPIRE=30d

# Environment
# Options: development, production
NODE_ENV=development